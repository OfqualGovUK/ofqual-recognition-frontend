@using Ofqual.Recognition.Frontend.Core.Constants

@{
    var controller = ViewContext.RouteData.Values["controller"]?.ToString();
}

<div class="govuk-button-group">

        <button type="submit" class="govuk-button" data-module="govuk-button">
            Save and continue
        </button>

        <a class="govuk-link" href="@(RouteConstants.ApplicationConstants.TASK_LIST_PATH)">Cancel</a>
    <input type="hidden" name="currentTaskNameUrl" value="@ViewData["CurrentTaskNameUrl"]" />
    <input type="hidden" name="currentQuestionNameUrl" value="@ViewData["CurrentQuestionNameUrl"]" />
    <a asp-controller="PreEngagement" asp-action="GetCache">show cache</a>
</div>