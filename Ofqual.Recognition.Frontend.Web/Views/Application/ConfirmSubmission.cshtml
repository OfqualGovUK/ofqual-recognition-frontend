@{
    var feedbackUrl = $"https://forms.office.com/Pages/ResponsePage.aspx?id=aWQzjmscC0ugbHSKfFhvfATsksjOCgNFm2yr1lfODUxUQVlHNUFXNEFRSjJBNTlKRUwzQ1lQVEVONSQlQCN0PWcu&rffa74bd1e4fe42d88ef9994c4304d570={ViewData["HeaderTitle"]}";
}

<div class="govuk-grid-row">
    <div class="govuk-grid-column-two-thirds">
        <div class="govuk-panel govuk-panel--confirmation">
            <h1 class="govuk-panel__title">
                Application complete
            </h1>                   
        </div>
        <p class="govuk-body">We have sent you a confirmation email.</p>
        <h2 class="govuk-heading-m">What happens next</h2>
        <p class="govuk-body">
            We’ve sent your application to Ofqual.
        </p>
        <p class="govuk-body">
            They will contact you either to confirm your registration, or to ask for more information.
        </p>
        <p class="govuk-body">
            <a href="@feedbackUrl" class="govuk-link">What did you think of this service?</a> (takes 1 minute)
        </p>
    </div>
</div>

