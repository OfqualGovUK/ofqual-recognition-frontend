{
  "ApiAuthentication": {
    "BaseUrl": "CITIZEN-API-URL"
    //"BaseUrl": "https://localhost:7037"
  },
  "LogzIo": {
    "Environment": "DEV",
    "BaseUrl": "LOGZ.IO-URL",
    "Token": "LOGZ.IO-SECRET"
  },
  "AzureAdB2C": {
    "Instance": "https://<tenant-name>.b2clogin.com/",
    "ClientId": "FRONTEND-APP-REGISTRATION",
    "ClientCredentials": [
      {
        "SourceType": "ClientSecret",
        "ClientSecret": "FRONTEND-APP-REGISTRATION-SECRET"
      }
    ],
    "Domain": "<tenant-domain>",
    "SignUpSignInPolicyId": "SUSI-POLICY",
    "SignedOutCallbackPath": "/signout/SUSI-POLICY",
    "ResetPasswordPolicyId": "RESET-POLICY",
    "CallbackPath": "/signin-oidc"
  },
  "DownstreamApis": {
    "CitizenAPI": {
      "BaseUrl": "https://localhost:7037",
      "Scopes": [ "SCOPE" ]
    }
  }
}